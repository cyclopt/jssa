{
  "name": "jssa",
  "version": "0.0.3",
  "description": "JS static analyzer (jssa): An aggregation of javascript source code static analysis tools",
  "main": "analysis.js",
  "dependencies": {
    "babel-eslint": "^8.2.3",
    "bluebird": "^3.5.1",
    "escomplex": "^2.0.0-alpha",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-native": "^3.2.1",
    "eslint-plugin-security": "^1.4.0",
    "esprima": "^4.0.0",
    "fs": "^0.0.1-security",
    "jsinspect": "^0.12.7",
    "lodash": "^4.17.4",
    "nsp": "^3.1.0",
    "shelljs": "^0.7.8",
    "sonarjs": "^1.0.0",
    "walk": "^2.3.9"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-coverage-badges": "^1.0.0"
  },
  "scripts": {
    "test": "node_modules/.bin/jest --coverage; node_modules/.bin/jest-coverage-badges; cp coverage/badge-lines.svg ."
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "**/*.{js}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/example_project/",
      "<rootDir>/examples/",
      "<rootDir>/coverage/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyclopt/jssa.git"
  },
  "author": "Cyclopt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyclopt/jssa/issues"
  },
  "homepage": "https://github.com/cyclopt/jssa#readme"
}
