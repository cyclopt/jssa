{
  "name": "jssa",
  "version": "0.0.5",
  "description": "JS static analyzer (jssa): An aggregation of javascript source code static analysis tools",
  "main": "analysis.js",
  "bin": {
    "jssa": "./bin/jssa-cli.js"
  },
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "bluebird": "^3.7.2",
    "comments-extractor": "^1.0.9",
    "escomplex": "github:cyclopt/escomplex#6f4bcde523ee6a83e582f9f9b7d4921f022f805a",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-native": "^3.8.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "esprima": "^4.0.1",
    "jsinspect": "github:cyclopt/jsinspect#f254892f530b9eea11c39a9da6942d95a9ba3af0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "run-npm-audit": "^1.0.2",
    "shelljs": "^0.8.3",
    "sonarjs": "^1.0.0",
    "walk": "^2.3.14"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-iamnapo": "^2.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jest-coverage-badges": "^1.1.2",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "test-windows": "jest --coverage & jest-coverage-badges & copy coverage\\badge-lines.svg badge-lines.svg",
    "test-linux": "jest --coverage; jest-coverage-badges; cp coverage/badge-lines.svg ."
  },
  "jest": {
    "verbose": true,
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "analyzers/*",
      "analysis.js",
      "utilities/*"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/example_project/",
      "<rootDir>/examples/",
      "<rootDir>/coverage/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyclopt/jssa.git"
  },
  "author": "Cyclopt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyclopt/jssa/issues"
  },
  "homepage": "https://github.com/cyclopt/jssa#readme"
}
